{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#3bb7ff",
          "style": "plain",
          "template": "\uf0e7 ",
          "type": "root"
        },
        {
          "background": "#000000",
          "foreground": "#ffffff",
          "leading_diamond": "<transparent,#000000>\ue0b0</>",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "#1E1E1E",
          "foreground": "#ffffff",
          "leading_diamond": "<#000000,#1E1E1E>\ue0b0</>",
          "properties": {
            "folder_icon": "\uf6d7",
            "folder_separator_icon": "<transparent> \ue0bd </>",
            "home_icon": "\uf7db",
            "style": "agnoster_short"
          },
          "style": "diamond",
          "template": " {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "#00C853",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#68217a",
          "foreground": "#FFFFFF",
          "powerline_symbol": "\uE0B0",
          "style": "powerline",
          "template": " \uE70c {{ .Full }} ",
          "type": "dotnet"
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "template": " \uE718 {{ .Full }} "
        },
        {
          "type": "npm",
          "style": "diamond",
          "leading_diamond": "<transparent,#C53635>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#C53635",
          "template": " \ue71e {{ .Full }} "
        },
        {
          "type": "deno",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#3C82F6",
          "leading_diamond": "<transparent,#3C82F6>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "template": " \ufbe4 {{ .Full }} "
        },
        {
          "type": "php",
          "style": "diamond",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#4063D8",
          "leading_diamond": "<transparent,#4063D8>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "template": " \ue73d {{ .Full }} "
        },
        {
          "type": "ytm",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#FF0000",
          "properties": {
            "playing_icon": "\uE602 ",
            "paused_icon": "\uF8E3 ",
            "stopped_icon": "\uF04D "
          }
        },
        {
          "background": "#48359E",
          "foreground": "#FFFFFF",
          "leading_diamond": "<transparent,#48359E>\ue0b0</>",
          "properties": {
            "time_format": "15:04"
          },
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b0",
          "type": "time"
        },
        {
          "background": "#49404f",
          "foreground": "#ffffff",
          "leading_diamond": "<transparent,#49404f>\ue0b0</>",
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }}s ",
          "trailing_diamond": "\ue0b0",
          "type": "executiontime"
        },
        {
          "background": "#910000",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": "<transparent> \uf12a</> {{ .Meaning }} ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#ffffff",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "[{{ .UserName }}@{{ .HostName }}:{{.Folder}}]$ "
  },
  "console_title_template": "{{if .Root}} \u26a1 {{end}}{{.Folder | replace \"~\" \"üèö\" }} @ {{.HostName}}",
  "osc99": true,
  "version": 2
}
